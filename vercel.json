{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "crons": [
    {
      "path": "/api/cron/weekly-reminders",
      "schedule": "0 9 * * 1"
    },
    {
      "path": "/api/cron/auto-feedback",
      "schedule": "0 2 * * *"
    }
  ],
  "functions": {
    "src/app/api/cron/weekly-reminders/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/auto-feedback/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/instructor/feedback/batch-generate/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/instructor/feedback/batch-generate-async/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/instructor/feedback/upload/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}